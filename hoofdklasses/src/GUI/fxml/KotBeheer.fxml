<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #99CCFF;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.ControllerKotBeheer">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="283.0" layoutY="-2.0" prefHeight="82.0" prefWidth="154.0" text="Locations">
         <font>
            <Font name="System Bold" size="34.0" />
         </font>
      </Label>
      <ListView fx:id="locationList" layoutX="30.0" layoutY="118.0" prefHeight="260.0" prefWidth="200.0" />
      <Button fx:id="addLocationBtn" layoutX="449.0" layoutY="316.0" mnemonicParsing="false" onAction="#addLocation" text="Add Location" />
      <Button fx:id="removeLocationBtn" layoutX="449.0" layoutY="352.0" mnemonicParsing="false" onAction="#removeLocation" text="Remove Location" />
      <Label fx:id="warningLabel1" layoutX="287.0" layoutY="343.0" />
      <TextField fx:id="streetTxt" layoutX="286.0" layoutY="118.0" onAction="#setStreet" promptText="Street" />
      <TextField fx:id="numberTxt" layoutX="287.0" layoutY="163.0" onAction="#setNumber" promptText="Number" />
      <TextField fx:id="cityTxt" layoutX="287.0" layoutY="215.0" onAction="#setCity" promptText="City" />
      <TextField fx:id="zipTxt" layoutX="287.0" layoutY="267.0" onAction="#setZIP" promptText="ZIP" />
      <CheckBox fx:id="insulatedCheck" layoutX="286.0" layoutY="308.0" mnemonicParsing="false" onAction="#isInsulated" prefHeight="26.0" prefWidth="150.0" text="Insulated" />
      <Label layoutX="30.0" layoutY="96.0" prefHeight="18.0" prefWidth="200.0" text="Your Locations" />
      <Button fx:id="homepageBtn" layoutX="30.0" layoutY="26.0" mnemonicParsing="false" onAction="#toHomepage" text="Back to the homepage" />
      <Label fx:id="warningLabel2" layoutX="287.0" layoutY="369.0" />
      <Label fx:id="warningLabel3" layoutX="449.0" layoutY="293.0" prefHeight="18.0" prefWidth="142.0">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
