<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.ControllerAddLocation">
   <children>
      <Label alignment="CENTER" layoutX="73.0" layoutY="40.0" prefHeight="40.0" prefWidth="454.0" text="Before you continue, please add a location first.">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="streetTxt" layoutX="45.0" layoutY="133.0" onAction="#setStreet" promptText="Street" />
      <TextField fx:id="numberTxt" layoutX="45.0" layoutY="234.0" onAction="#setNumber" promptText="Number" />
      <TextField fx:id="cityTxt" layoutX="358.0" layoutY="133.0" onAction="#setCity" promptText="City" />
      <TextField fx:id="ZIPtxt" layoutX="358.0" layoutY="234.0" onAction="#setZIP" promptText="ZIP" />
      <CheckBox fx:id="insulatedCheckBox" layoutX="228.0" layoutY="321.0" mnemonicParsing="false" onAction="#isInsulated" text="Insulated" />
      <Label layoutX="45.0" layoutY="116.0" text="Street" />
      <Label layoutX="45.0" layoutY="216.0" text="Number" />
      <Label layoutX="358.0" layoutY="116.0" text="City" />
      <Label layoutX="358.0" layoutY="216.0" text="ZIP" />
      <Button fx:id="addLocationBtn" layoutX="421.0" layoutY="317.0" mnemonicParsing="false" onAction="#addLocation" text="Add Location" />
   </children>
</AnchorPane>
